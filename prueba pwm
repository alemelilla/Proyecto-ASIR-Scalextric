int EN1=12;
int IN1=42;
int IN2=43;

void setup() {
  pinMode(EN1,OUTPUT);
  pinMode(IN1,OUTPUT);
  pinMode(IN2,OUTPUT);
  delay(500);

}

void loop() {
  digitalWrite(IN1,LOW);
  digitalWrite(IN2,HIGH);
  analogWrite(EN1,255);
  delay(1000);
  analogWrite(EN1,0);
  delay(1000);
  analogWrite(EN1,255);
  delay(1000);
  analogWrite(EN1,0);
  

}
